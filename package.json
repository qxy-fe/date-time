{
  "name": "@qxy/date-time",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@9.12.2",
  "description": "QXY-FE date time utils for OA projects",
  "keywords": [
    "qxy",
    "date",
    "time",
    "dayjs"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/qxy-fe/date-time#readme",
  "bugs": {
    "url": "https://github.com/qxy-fe/date-time/issues"
  },
  "repository": "qxy-fe/date-time",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s typecheck lint",
    "release:publish": "pnpm publish --access public",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "vitest --typecheck"
  },
  "peerDependencies": {
    "dayjs": "^1.11.7"
  },
  "dependencies": {
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.1.0",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/node": "^20.16.13",
    "@vitest/coverage-v8": "^2.1.3",
    "bumpp": "^9.7.1",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.1",
    "pnpm": "^9.12.2",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "unbuild": "^3.0.0-rc.11",
    "vitest": "^2.1.3"
  },
  "prettier": "@ntnyq/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,yml,yaml,md,json}": "eslint --fix"
  }
}
