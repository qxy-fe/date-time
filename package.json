{
  "name": "@qxy/date-time",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@8.15.4",
  "description": "QXY-FE date time utils for OA projects",
  "keywords": [
    "qxy",
    "date",
    "time",
    "dayjs"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/qxy-fe/date-time#readme",
  "bugs": {
    "url": "https://github.com/qxy-fe/date-time/issues"
  },
  "repository": "qxy-fe/date-time",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s typecheck lint",
    "release:publish": "pnpm publish --access public",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "vitest --typecheck"
  },
  "peerDependencies": {
    "dayjs": "^1.11.7"
  },
  "dependencies": {
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.20.3",
    "@ntnyq/prettier-config": "^1.20.3",
    "@types/node": "^20.11.25",
    "@vitest/coverage-v8": "^1.3.1",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.1.2",
    "pnpm": "^8.15.4",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.3.1"
  },
  "prettier": "@ntnyq/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,yml,yaml,md,json}": "eslint --fix"
  }
}
